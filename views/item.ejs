<tr>
    <th scope="row"><%= i+1 %></th>
    <td><%= car.carName %></td>
    <td><img style="width: 50px; height: 50px" src="<%= car.images %>"></td>
    <td><%= car.quantity %></td>
    <td><%= car.price %></td>
    <td><%= car.date %></td>
    <td class="align-middle text-center">
        <a href="javascript:;" class="text-secondary font-weight-bold text-xs" data-id="<%= car._id %>" data-bs-toggle="modal" data-bs-target="#editCarModal">
            Edit
        </a>
        &#160
        <a href="javascript:;" class="btn_delete_car text-secondary font-weight-bold text-xs text-danger" data-id="<%= car._id %>" data-bs-toggle="modal" data-bs-target="#deleteProductModal">
            Delete
        </a>
    </td>
</tr>